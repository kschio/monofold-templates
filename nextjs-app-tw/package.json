{
  "name": "monofold-nextjs-app-tw",
  "description": "Opinionated Next.js app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "yarn rimraf .next",
    "build": "next build",
    "dev": "next dev",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "lint:quiet": "eslint --quiet \"src/**/*.{ts,tsx}\"",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "yarn playwright test",
    "test:e2e:report": "yarn playwright show-report"
  },
  "peerDependencies": {
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies": {
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.21",
    "tailwindcss": "3.3.1"
  },
  "devDependencies": {
    "monofold-eslint-pkg": "*",
    "monofold-tsconfig-pkg": "*",
    "@playwright/test": "^1.32.3",
    "@types/eslint": "^8.37.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.30.1",
    "@vitest/ui": "0.30.1",
    "eslint": "8.38.0",
    "playwright": "^1.32.3",
    "rimraf": "^5.0.0",
    "typescript": "5.0.4",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "^0.30.1",
    "vitest-fetch-mock": "0.2.2"
  },
  "engines": {
    "npm": "only-use-yarn"
  }
}
