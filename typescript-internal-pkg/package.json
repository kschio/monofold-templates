{
  "name": "monofold-typescript-internal-pkg",
  "description": "Opinionated TypeScript internal package",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "lint": "TIMING=1 eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "lint:quiet": "eslint --quiet \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "monofold-eslint-pkg": "*",
    "monofold-tsconfig-pkg": "*",
    "@types/eslint": "^8.44.2",
    "@types/node": "20.5.0",
    "@vitest/coverage-v8": "0.34.2",
    "@vitest/ui": "0.34.2",
    "eslint": "8.47.0",
    "rimraf": "^5.0.1",
    "typescript": "5.1.6",
    "vite": "^4.4.9",
    "vitest": "0.34.2"
  }
}
